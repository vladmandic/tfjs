{
  "name": "@vladmandic/tfjs",
  "version": "4.5.0-20230508",
  "description": "Custom build of Tensorflow/JS for Browsers as pure ESNext",
  "sideEffects": false,
  "main": "dist/tfjs.esm.js",
  "module": "dist/tfjs.esm.js",
  "browser": "dist/tfjs.esm.js",
  "types": "types/tfjs.d.ts",
  "scripts": {
    "clean": "rimraf .tfjs* src dist/* types/*",
    "download": "scripts/download.sh",
    "wasm": "scripts/wasm.sh",
    "bundle": "node scripts/bundle.js",
    "types": "rimraf types/lib && scripts/types.sh",
    "build": "npm run download && npm run wasm && npm run bundle && npm run types",
    "commit": "git status --short && git commit --all --file .tfjs-version && git push && git log --max-count=1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/tfjs.git"
  },
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/tfjs/issues"
  },
  "homepage": "https://github.com/vladmandic/tfjs#readme",
  "keywords": [
    "tensorflowjs",
    "tfjs"
  ],
  "devDependencies": {
    "@microsoft/api-extractor": "^7.34.8",
    "@types/argparse": "^2.0.10",
    "@types/emscripten": "0.0.34",
    "@types/estree": "^1.0.1",
    "@types/jasmine": "~4.3.1",
    "@types/js-yaml": "^4.0.5",
    "@types/long": "4.0.0",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.16.5",
    "@types/node-fetch": "~2.6.3",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@types/seedrandom": "^3.0.5",
    "@types/shelljs": "^0.8.12",
    "@types/webgl-ext": "0.0.32",
    "@types/webgl2": "0.0.6",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vladmandic/build": "^0.7.15",
    "@vladmandic/pilogger": "^0.4.8",
    "@webgpu/types": "0.1.21",
    "esbuild": "^0.16.17",
    "eslint": "8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "rimraf": "^3.0.2",
    "seedrandom": "^3.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs-backend-cpu": "link:src/tfjs-backend-cpu",
    "@tensorflow/tfjs-backend-wasm": "link:src/tfjs-backend-wasm",
    "@tensorflow/tfjs-backend-webgl": "link:src/tfjs-backend-webgl",
    "@tensorflow/tfjs-backend-webgpu": "link:src/tfjs-backend-webgpu",
    "@tensorflow/tfjs-converter": "link:src/tfjs-converter",
    "@tensorflow/tfjs-core": "link:src/tfjs-core",
    "@tensorflow/tfjs-data": "link:src/tfjs-data",
    "@tensorflow/tfjs-layers": "link:src/tfjs-layers"
  }
}
